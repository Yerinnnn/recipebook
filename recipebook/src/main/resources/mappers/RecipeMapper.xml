<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.recipebook.mappers.recipe">

	<!-- 게시물 목록 -->
	<select id="list" parameterType="list"
		resultType="com.recipebook.domain.RecipeVO">
		SELECT
		    recipe_id, 
		    title, 
		    sub_title, 
		    food_name, 
		    username, 
		    date, 
		    time,
            content, 
            hits,
		    thumbImg
		FROM recipe

	</select>


</mapper>